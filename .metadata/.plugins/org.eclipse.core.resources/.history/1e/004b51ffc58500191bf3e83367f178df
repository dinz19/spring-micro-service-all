  jpa: 
     hibernate: 
       ddl-auto: update 
     show-sql: true
     properties:
      hibernate.generate_statistics : true
  application:
    name: inventory-microservice

eureka:  
  instance:
    hostname: localhost
  client:
    register-with-eureka : true
    fetch-registry : true
    service-url:
      defaultZone : http://${eureka.instance.hostname}:1234/eureka 
   
  
   
  
  
        