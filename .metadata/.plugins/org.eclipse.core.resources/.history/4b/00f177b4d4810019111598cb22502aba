package com.cts.product.main.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class Product {
	
	@Id
	private long id;
	
	@Column
	private String name;
	
	@Column
	private String price;
	
	@Column
	private boolean stockStatus;
	
	

}
